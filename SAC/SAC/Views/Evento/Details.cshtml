@model SAC.Models.EventoModelView

@{
    ViewBag.Title = "Detalles";
}


<div class="card-body p-t-0">
    <div class="card b-all shadow-none">
        <div class="card-body">
            <h3 class="card-title m-b-0">Detalles de Evento</h3>
        </div>
        <div>
            <hr class="m-t-0">
        </div>
        <div class="card-body">

            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label">  Tipo: </label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">   @Html.DisplayFor(model => model.Tipo)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Fecha / hora:</label>
                <div class="col-4">
                    <label for="example-text-input" class="col-form-label">   @Html.DisplayFor(model => model.FechaInicio)</label>
                </div>
                <div class="col-4">
                    <label for="example-text-input" class="col-form-label">   @Html.DisplayFor(model => model.FechaFin)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Asisten:</label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">         @Html.DisplayFor(model => model.Asisten)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Pasado A:</label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">         @Html.DisplayFor(model => model.Pasado)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Recibido:</label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">         @Html.DisplayFor(model => model.Recibido)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Organizador:</label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">         @Html.DisplayFor(model => model.Organizador)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Ubicación:</label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">         @Html.DisplayFor(model => model.Ubicacion)</label>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-text-input" class="col-4 col-form-label"> Obs.:</label>
                <div class="col-8">
                    <label for="example-text-input" class="col-form-label">         @Html.DisplayFor(model => model.Obs)</label>
                </div>
            </div>
        </div>
      
    </div>
</div>

<p>
   
    <a href="@Url.Action("Index", "Evento")" class="btn btn-inverse m-t-20"> Volver </a>
    
   
</p>
